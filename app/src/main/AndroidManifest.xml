<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apkbus.materialdesigndemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".snackbar.SnackBarActivity" />
        <activity android:name=".bottomnavigation.BottomNavigationActivity" />
        <activity android:name=".toolbar.ToolBarActivity" />
        <activity android:name=".navigationview.NavigationViewActivity" />
        <activity android:name=".floatingactionbutton.FloatingActionButtonActivity" />
        <activity android:name=".coordinatorlayout.CoordingatorLayoutActivity" />
        <activity android:name=".appbarlayout.AppBarLayoutActivity" />
        <activity android:name=".tablayout.TabLayoutActivity" />
        <activity android:name=".textinputlayout.TextInputLayoutActivity" />
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>